<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    <application>
    	<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>el</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>com.mpdmal.cloudental.cloudental</base-name>
			<var>msgs</var>
		</resource-bundle>
	</application>
	<converter>
		<converter-id>patientConverter</converter-id>
		<converter-class>com.mpdmal.cloudental.web.converter.PatientConverter</converter-class>
	</converter>
	<converter>
		<converter-id>activityConverter</converter-id>
		<converter-class>com.mpdmal.cloudental.web.converter.ActivityConverter</converter-class>
	</converter>
	<converter>
		<converter-id>pricelistItemConverter</converter-id>
		<converter-class>com.mpdmal.cloudental.web.converter.PricelistItemConverter</converter-class>
	</converter>
	<converter>
		<converter-id>discountConverter</converter-id>
		<converter-class>com.mpdmal.cloudental.web.converter.DiscountConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>dentistManagementBean</managed-bean-name>
		<managed-bean-class>com.mpdmal.cloudental.web.beans.DentistManagementBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginRequest</managed-bean-name>
		<managed-bean-class>com.mpdmal.cloudental.web.beans.LoginRequest</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>userHolder</managed-bean-name>
		<managed-bean-class>com.mpdmal.cloudental.web.beans.UserHolder</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>patientManagementBean</managed-bean-name>
		<managed-bean-class>com.mpdmal.cloudental.web.beans.PatientManagementBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>medicalHistoryManagementBean</managed-bean-name>
		<managed-bean-class>com.mpdmal.cloudental.web.beans.MedicalHistoryManagementBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>scheduleController</managed-bean-name>
		<managed-bean-class>com.mpdmal.cloudental.web.beans.ScheduleController</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>scheduleBean</managed-bean-name>
		<managed-bean-class>com.mpdmal.cloudental.web.beans.ScheduleBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<!-- managed-bean>
		<managed-bean-name>autoCompleteBean</managed-bean-name>
		<managed-bean-class>com.mpdmal.cloudental.web.beans.AutoCompleteBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean -->
	<managed-bean>
		<managed-bean-name>activityManagementBean</managed-bean-name>
		<managed-bean-class>com.mpdmal.cloudental.web.beans.ActivityManagementBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>visitManagementBean</managed-bean-name>
		<managed-bean-class>com.mpdmal.cloudental.web.beans.VisitManagementBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pricelistItemManagementBean</managed-bean-name>
		<managed-bean-class>com.mpdmal.cloudental.web.beans.PricelistItemManagementBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>discountManagementBean</managed-bean-name>
		<managed-bean-class>com.mpdmal.cloudental.web.beans.DiscountManagementBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>LoginRule</display-name>
		<from-view-id>/screens/frontend/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>loggedIn</from-outcome>
			<to-view-id>/screens/frontend/PatientManagement.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
